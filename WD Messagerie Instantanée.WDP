     internal_properties : 
     code : |1+
      GLOBAL	
      	CONSTANT
      		// Nom des sockets utilisées
      		csNomSocketPresence = "SocketPresence"			// Socket permettant de détecter la présence d'un utilisateur
      		csNomSocketRequete = "SocketRequete"			// Socket permettant d'effectuer des demandes de présence
      		
      		// Numéros de ports associés aux sockets
      		cnPortSocketPresence = 5010					// Port associé à la socket csNomSocketPresence
      		cnPortSocketRequete = 5011					// Port associé à la socket csNomSocketRequete
      		
      		// Message du protocole
      		csACKOK = "OK"								// Message de confirmation
      		csNONDISPO = "NON_DISPO"						// Utilisateur non disponible
      		csFINCOM = "DEMANDE_ARRET"					// Demande de fin de communication
      		csREQ = "REQ"								// Requete de connexion
      	FIN
      
      hgererep(faux)
      
      //
      // Transfert des fichiers en local si on est sur un disque réseau
      //
      si fDisqueInfo(fExtraitChemin(frepexe(),fDisque),fdTypeDisque) = fdRéseau ou RéseauNomRep(fExtraitChemin(frepexe(),fDisque)) <> "" alors
      		
      	// génère un répertoire local
      	gsRepFichiers est une chaine = completerep(sysrep(srAppDataLocal)) + ProjetInfo(piConfiguration) + "\"
      	
      	// crée le répertoire
      	frepcree(gsRepFichiers)
      	
      	// Modifie le chemin des fichiers de données
      	HSubstRep("*", gsRepFichiers)
      FIN
      
      // Création du fichier des contacts si nécessaire
      HCréationSiInexistant(CONTACT)						
      
      // Ouverture de la socket de présence
      OuvertureSocketPresence()
      
      // On gère les threads en manuels (chaque procédure n'est pas une section critique)
      ThreadMode(threadSectionCritique)
